<Window x:Class="Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,0">
        <Grid Name="HeaderGrid" HorizontalAlignment="Left" Height="35" Margin="0,0,0,0" VerticalAlignment="Top" Width="791"/>
        <Grid Name="ContentGrid" HorizontalAlignment="Left" Height="345" Margin="0,35,0,0" VerticalAlignment="Top" Width="791">
            <ScrollViewer Name="ContentPane" HorizontalAlignment="Left" Height="345" VerticalAlignment="Top" Width="791">
                <Grid Name="ContentInlineGrid" HorizontalAlignment="Left" Height="345" Margin="0,35,0,0" VerticalAlignment="Top" Width="791"></Grid>
            </ScrollViewer>
        </Grid>
        <Grid Name="FooterGrid" HorizontalAlignment="Left" Height="35" Margin="0,380,0,0" VerticalAlignment="Top" Width="791"/>

    </Grid>

</Window>
